%tbody.complete{id: "list_item_#{todo_item.id}"}
  - if todo_item.completed?
    %tr.complete-item.items
      %td
        = link_to complete_shopquik_todo_list_shopquik_todo_item_path(@todo_list, todo_item.id), method: :patch, remote: true do
          %i.fa.fa-check-square-o{"data-toggle" => "tooltip", "data-placement" => "top", title: "Item Completed"}
      %td.text-left= todo_item.item
      %td= todo_item.quantity
      %td= todo_item.cost
      %td
        = link_to shopquik_todo_list_shopquik_todo_item_path(@todo_list, todo_item.id), method: :delete, data: { confirm: "Are you sure?" }, remote: true do
          %i.fa.fa-trash{"data-toggle" => "tooltip", "data-placement" => "top", title: "Delete Item"}
  -else
    %tr.items
      %td
        = link_to complete_shopquik_todo_list_shopquik_todo_item_path(@todo_list, todo_item.id), method: :patch, remote: true do
          %i.fa.fa-check-square-o{"data-toggle" => "tooltip", "data-placement" => "top", title: "Mark As Complete"}
      %td.text-left= todo_item.item
      %td= todo_item.quantity
      %td= todo_item.cost
      %td
        = link_to shopquik_todo_list_shopquik_todo_item_path(@todo_list, todo_item.id), method: :delete, data: { confirm: "Are you sure?" }, remote: true do
          %i.fa.fa-trash{"data-toggle" => "tooltip", "data-placement" => "top", title: "Delete Item"}

:javascript
  $(document).ready(function() {
    $('[data-toggle="tooltip"]').tooltip();
  });